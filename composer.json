{
  "name": "chocofamilyme/laravel-clickhouse-migrations",
  "description": "Clickhouse migrations based on Laravel migrations and smi2/phpclickhouse",
  "keywords": [
    "laravel",
    "clickhouse"
  ],
  "type": "library",
  "require": {
    "php": "^8.0",
    "smi2/phpclickhouse": "^1.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chocofamilyme\\LaravelClickHouse\\Migrations\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "IT Chocolife.me",
      "homepage": "https://chocolife.me"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Chocofamilyme\\LaravelClickHouse\\Migrations\\Providers\\ClickHouseMigrationsServiceProvider"
      ]
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes --config=.php_cs.dist.php"
  }
}
